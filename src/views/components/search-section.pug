#search-section
    h2.section-title Solusi lengkap untuk kebutuhan pasokan bisnis Anda, dari bahan baku hingga produk jadi.
    .search-bar
        input(
            type='text', 
            placeholder='Masukkan nama perusahaan', 
            id='search-input'
        )
        button#search-button(onclick='showSearchResult()')
            span.material-symbols--search Cari
#search-modal
    .search-modal-content
        button#search-modal-tutup(onclick='closeSearchResult()') Tutup
        h2#search-modal-title Hasil Pencarian
        .search-supplier-list
            div.loader(id="search-loader")
        